package JavaFundamentals;

public class RangeOfAllDataType
{
	static char c=300;

	public static void main(String[] args)
	{
		System.out.println("Range of Byte = "+Byte.MAX_VALUE);
		System.out.println("Range of Short = "+Short.MAX_VALUE);
		System.out.println("Range of Integer = "+Integer.MAX_VALUE);
		System.out.println("Range of Long = "+Long.MAX_VALUE);
		System.out.println("Range of Float = "+Float.MAX_VALUE);
		System.out.println("Range of Double = "+Double.MAX_VALUE);
		System.out.println("Range of Char = "+(Character.MAX_VALUE+0));
		System.out.println("Range of Char = "+c);
		System.out.println("Range of Boolean = "+Boolean.FALSE);

	}

}
